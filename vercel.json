{
      "version": 2,
      "builds": [
        {
          "src": "src/main.rs",
          "use": "@vercel/rust",
          "config": {
            "buildCommand": "cargo build --release",
            "outputDirectory": "target/release"
          }
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "@vercel/rust"
        }
      ]
    }